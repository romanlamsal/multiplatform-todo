{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true,
    "moduleResolution": "NodeNext"
  },
  "include": ["src/**/*.ts", "src/**/*.js", "src/**/*.svelte"],
}
